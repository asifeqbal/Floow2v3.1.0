<?xml version='1.0' encoding='utf-8'?>
<widget id="com.robhaenen.floow2" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>FLOOW2</name>
    <description>
        FLOOW2 World's reset button
    </description>
    <author email="rob.haenen@floow2.com" href="http://www.floow2.com/fl2-en-mobile.html">
        FLOOW2 Team
    </author>
    <content src="index.html" />
	<plugin name="phonegap-plugin-push" source="npm" spec="~1.8.0">
        <variable name="SENDER_ID" value="114063791200" />
    </plugin>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin browserOnly="true" name="cordova-plugin-inappbrowser" spec="~1.7.1" />
    <plugin name="cordova-plugin-splashscreen" spec="~4.0.3" />
    <plugin name="cordova-plugin-globalization" spec="1.0.7" />
    <plugin name="cordova-plugin-network-information" spec="1.3.3" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="phonegap-plugin-push" spec="~2.0.0-rc5" />
</widget>
